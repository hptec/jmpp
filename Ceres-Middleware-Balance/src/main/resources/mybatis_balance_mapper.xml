<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cerestech.middleware.balance.mapper.BalanceMapper">
	
	<select id="search" resultType="cn.cerestech.middleware.balance.entity.BalanceAccount">
		SELECT * FROM `$$balance_account` ba 
		WHERE 
			ba.owner_id = #{owner.owner_id}
		AND 
			ba.owner_type = #{owner.owner_type}
	</select>
</mapper>